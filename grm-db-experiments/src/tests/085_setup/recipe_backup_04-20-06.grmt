	<recipe id="r185">
		<title>
			Tuna Casserole
		</title>
		<servings>
			4
		</servings>
		<source>
			Sarah Gilbert
		</source>
		<image format="jpeg">
<![CDATA[]]>		</image>
		<ingredient-list>
			<ingredient>
				<amount>
					1/2
				</amount>
				<unit>
					lb.
				</unit>
				<item>
